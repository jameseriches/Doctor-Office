<h1>Set New Appointment</h1>

<%= form_for @appointment do |f| %>
  <%= f.label :time %>
  <%= f.text_field :time %>
<br>
<br>
  <%= f.label :date %>
  <%= f.text_field :date %>
<br>

<br>
  <% f.label :doctors %> Doctor:
  <%= f.text_field :doctors %>
<br>
<br>
  <% f.label :user %> Patient:
  <%= f.text_field :users %>
  
  <%= f.submit %>

<% end %>